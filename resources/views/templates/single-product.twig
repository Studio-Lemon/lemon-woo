{% extends 'layouts/app.twig' %}
{% import 'macros/socials.twig' as socials %}
{% block content %}
    {{ timber_set_product(post) }}
    <article class="entry">
        {% do action('woocommerce_before_main_content') %}
        {{ fn('wc_get_template_part', 'content', 'single-product') }}
